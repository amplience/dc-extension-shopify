{
  "name": "amplience-shopify-extension",
  "version": "1.0.0",
  "description": "",
  "author": "The Maze Group",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=16.14"
  },
  "nodemonConfig": {
    "exec": "npm-run-all compile serve:dev",
    "ext": "js,json,ts,tsx",
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      [
        "src/*"
      ]
    ]
  },
  "scripts": {
    "clean": "rm -rf .next dist",
    "lint": "next lint",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --quiet --fix",
    "prepare": "husky install",
    "pre-commit": "npm run lint",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "compile:only": "npm-run-all clean compile",
    "compile": "tsc",
    "next:build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "serve:dev": "cross-env NODE_ENV=development ts-node dist/server/server.js",
    "dev": "nodemon",
    "next:export": "next export",
    "export": "npm-run-all clean next:build next:export",
    "build": "npm-run-all clean compile next:build",
    "build-prod": "npm-run-all clean build start",
    "docker:db": "docker compose -f docker-compose.db.yml up",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "start": "ts-node dist/server/server.js",
    "prod:start": "pm2 start ecosystem.config.json"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/lib-dynamodb": "^3.58.0",
