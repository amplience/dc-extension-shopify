    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "apollo-boost": "^0.4.9",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "babel-register": "^6.26.0",
    "chalk": "^5.0.1",
    "cli-color": "^2.0.2",
    "cors": "^2.8.5",
